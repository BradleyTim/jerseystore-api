{
  "version": 2,
  "name": "seasonstock-api",
  "builds": [
    {
      "src": "src/app.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/app.js"
    }
  ],
  "alias": [
    "jerseystore-api"
  ],
  "env": {
    "PORT": "@port",
    "MONGO_URI": "@mongo_uri",
    "CLOUD_NAME": "@cloud_name",
    "API_KEY": "@api_key",
    "API_SECRET": "@api_secret"
  }
}